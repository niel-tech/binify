{
  "name": "binify",
  "description": "creates secure bin's on the fly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env FORCE_COLOR=1 NEXTAUTH_URL=http://localhost:3000 next dev",
    "prebuild": "node prebuild.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true yarn build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "test": "cross-env FORCE_COLOR=1 jest --passWithNoTests",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md,js}\"",
    "postinstall": "npx patch-package -y",
    "preinstall": "npx only-allow yarn",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg",
    "fauna-cloud-schema:sync": "fgu --config .fauna.json",
    "fauna-cloud-schema:override": "fgu --config .fauna.json --mode override",
    "fauna-cloud-schema:dev:sync": "fgu --config .dev.fauna.json",
    "fauna-cloud-schema:dev:override": "fgu --config .dev.fauna.json --mode override",
    "fauna-cloud:migration:state": "env-cmd fauna-schema-migrate state",
    "fauna-cloud:migration:apply": "env-cmd fauna-schema-migrate apply all",
    "fauna-cloud:dev:migration:state": "env-cmd cross-env FAUNA_CHILD_DB=dev fauna-schema-migrate state",
    "fauna-cloud:dev:migration:apply": "env-cmd cross-env FAUNA_CHILD_DB=dev fauna-schema-migrate apply all",
    "fauna-migration:migrate": "fauna-schema-migrate generate"
  },
  "dependencies": {
    "@auth/fauna-adapter": "^1.0.0",
